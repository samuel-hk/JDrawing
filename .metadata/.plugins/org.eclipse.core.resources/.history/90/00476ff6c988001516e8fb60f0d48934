import java.awt.geom.Line2D;

public class A {

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		Line2D.Double line = new Line2D.Double(1.0, 1.0, 1.0, 1.0);
		Object lineObj = line;
		
		System.out.println("Class type : " + Line2D.Double.class == lineObj.getClass().toString());
	}

}
